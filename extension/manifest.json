{
  "manifest_version": 2,

  "name": "earthlive  ",
  "description": "This extension displays natural disasters on Google Maps",
  "version": "1.0",
  "content_scripts": [{
   "matches" : ["https://www.facebook.com/"],
   "js": ["jquery.js", "emoji2.js"]
  }],
  "web_accessible_resources": ["img/*"],
  "browser_action": {
    "default_icon": "img/emoji.png",
    "default_popup": "popup.html"
  },
  "background":
  {
    "scripts": ["jquery.js"]
  },
  "permissions": [
    "*://*.projectoxford.ai/*"
  ]
}
